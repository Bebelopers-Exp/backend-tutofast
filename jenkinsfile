pipeline {
  agent any
  tools{
    maven 'MAVEN_3_6_3'
    jdk 'JDK_1_11'
    }
    stages {
      stage('Compile Stage'){
        steps{
          withMaven(maven:'MAVEN_3_6_3'){
            bat'mvn clean compile'
            }
          }
        }
        stage('Testing Stage'){
          steps{
            withMaven(maven: 'MAVEN_3_6_3'){
              bat'mvn test'
              }
            }
          }

        stage('sonarQube Quality'){
          steps{
            withSonarQubeEnv('sonarQube'){
              bat'mvn clean verify sonar:sonar'
              }
            }
          }
        stage("package Stage"){
            steps{
              withMaven(maven: 'MAVEN_3_6_3'){
                bat 'mvn package'
                }
              }
            }

      }
  }
